<section id="archive">
  <div class="container">
    <ul class="list-group">
      <% var year = 0 %>
      <% var change = false %>
      <% page.posts.sort('date', 'desc').each(function(post) { %>
        <li class="list-group-item">
          <p class="mb-0">
            <span>
              <%- partial('_partial/post/title', { post: post, index: true, class_name: '' }) %>
            </span>
            - <%- partial('_partial/post/date', { post: post }) %>
          </p>
        </li>
      <% }); %>
    </ul>
  </div>
</section>

<section id="pagination">
  <div class="container">
    <%- partial('_partial/pagination') %>
  </div>
</section>
